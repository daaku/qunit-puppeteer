{
  "name": "@daaku/qunit-puppeteer",
  "author": "Naitik Shah <n@daaku.org>",
  "version": "3.1.0",
  "description": "Run QUnit tests using puppeteer.",
  "repository": "git@github.com:daaku/qunit-puppeteer",
  "bin": "./bin.mjs",
  "prettier": {
    "arrowParens": "avoid",
    "semi": false,
    "singleQuote": true,
